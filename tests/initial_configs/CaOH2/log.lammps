#creating the FCC lattice in a cubic region in the box

lattice 	fcc $(v_scale)
lattice 	fcc 0.60599732400958894463
Lattice spacing in x,y,z = 0.605997 0.605997 0.605997
variable 	xl equal xlo+0.05*$(v_xbsize)
variable 	xl equal xlo+0.05*6.0599732400958901124
variable 	xr equal xhi-0.05*$(v_xbsize)
variable 	xr equal xhi-0.05*6.0599732400958901124
variable 	yl equal ylo+0.05*$(v_ybsize)
variable 	yl equal ylo+0.05*6.0599732400958901124
variable 	yr equal yhi-0.05*$(v_ybsize)
variable 	yr equal yhi-0.05*6.0599732400958901124
variable 	zl equal zlo+0.05*$(v_zbsize)
variable 	zl equal zlo+0.05*6.0599732400958901124
variable 	zr equal zhi-0.05*$(v_zbsize)
variable 	zr equal zhi-0.05*6.0599732400958901124
region 		r_solid block $(v_xl) $(v_xr) $(v_yl) $(v_yr) $(v_zl) $(v_zr) units box
region 		r_solid block 0.30299866200479502742 $(v_xr) $(v_yl) $(v_yr) $(v_zl) $(v_zr) units box
region 		r_solid block 0.30299866200479502742 5.7569745780910999144 $(v_yl) $(v_yr) $(v_zl) $(v_zr) units box
region 		r_solid block 0.30299866200479502742 5.7569745780910999144 0.30299866200479502742 $(v_yr) $(v_zl) $(v_zr) units box
region 		r_solid block 0.30299866200479502742 5.7569745780910999144 0.30299866200479502742 5.7569745780910999144 $(v_zl) $(v_zr) units box
region 		r_solid block 0.30299866200479502742 5.7569745780910999144 0.30299866200479502742 5.7569745780910999144 0.30299866200479502742 $(v_zr) units box
region 		r_solid block 0.30299866200479502742 5.7569745780910999144 0.30299866200479502742 5.7569745780910999144 0.30299866200479502742 5.7569745780910999144 units box
create_atoms 	1 region r_solid
